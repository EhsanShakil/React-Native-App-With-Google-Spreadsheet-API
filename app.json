{
  "expo": {
    "name": "Google SpreadSheet API",
    "slug": "react-native-with-google-spreadsheet-api",
    "description": "How to create google spreadsheet API and build a react-native application.",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.ehsanshakil.reactnativewithgooglespreadsheetapi"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
